{
  "name": "citation-sort-hint",
  "displayName": "citation-sort-hint",
  "description": "reduce unnecessary information for organizing reference list",
  "publisher": "awtnb",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-citation-sort-hint"
  },
  "icon": "images/logo.png",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Reference-list",
    "Citation",
    "Highlight",
    "Focus",
    "Dim"
  ],
  "activationEvents": [
    "onCommand:citation-sort-hint.focus",
    "onCommand:citation-sort-hint.reset"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "citation-sort-hint.focus",
        "title": "focus",
        "category": "citation-sort-hint"
      },
      {
        "command": "citation-sort-hint.reset",
        "title": "reset",
        "category": "citation-sort-hint"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
